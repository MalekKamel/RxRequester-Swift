  use_frameworks!
  pod 'Moya/RxSwift', '~> 13.0.0'

  target 'RxRequesterMoyaTests' do
    pod 'RxTest'
    pod 'RxBlocking'
    pod 'Quick'
    pod 'Nimble'
  end

