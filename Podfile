end

target 'MoyaExample' do
  use_frameworks!

  pod 'RxSwift','~> 4.0'
  pod 'SwiftMessages', '6.0.1'
  pod 'Moya/RxSwift', '~> 13.0.0'

end

target 'RxRequester' do
  use_frameworks!

  pod 'RxSwift', '~> 4.0'

  target 'RxRequesterTests' do
    pod 'RxTest'
    pod 'RxBlocking'
    pod 'Quick'
    pod 'Nimble'
  end

end

